[32m+[m[32mpublic class Card {[m
[32m+[m	[32mprivate int senderAccNo;[m
[32m+[m	[32mprivate int recieverAccNo;[m
[32m+[m	[32mprivate int cvv;[m
[32m+[m	[32mprivate String transType;[m
[32m+[m	[32mprivate int ammount;[m
[32m+[m	[32mpublic Card() {[m
[32m+[m[41m		[m
[32m+[m	[32m}[m
[32m+[m	[32mpublic Card(int senderAccNo, int recieverAccNo, int cvv, String transType, int ammount) {[m
[32m+[m		[32msuper();[m
[32m+[m		[32mthis.senderAccNo = senderAccNo;[m
[32m+[m		[32mthis.recieverAccNo = recieverAccNo;[m
[32m+[m		[32mthis.cvv = cvv;[m
[32m+[m		[32mthis.transType = transType;[m
[32m+[m		[32mthis.ammount = ammount;[m
[32m+[m	[32m}[m
[32m+[m	[32mpublic int getSenderAccNo() {[m
