[32m+[m				[32mHttpSession session=request.getSession();[m
[32m+[m				[32msession.setAttribute("invalid", str1);[m
[32m+[m				[32mresponse.sendRedirect("Login.jsp");[m
[32m+[m
[32m+[m			[32m}[m
[32m+[m			[32melse if(!c.getPass().equals(pass)) {[m
