[32m+[m[32mimport project.dao.BankDao;[m
[32m+[m[32mimport project.dao.BankDaoImpl;[m
[32m+[m[32mimport project.model.Card;[m
[32m+[m[32mimport project.model.Customer;[m
[32m+[m[32mimport project.model.Rtgs;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Servlet implementation class CardPaymentController[m
[32m+[m[32m */[m
[32m+[m[32m@WebServlet("/CardPaymentController")[m
[32m+[m[32mpublic class CardPaymentController extends HttpServlet {[m
[32m+[m	[32mprivate static final long serialVersionUID = 1L;[m
[32m+[m[41m       [m
[32m+[m[32m    /**[m
[32m+[m[32m     * @see HttpServlet#HttpServlet()[m
[32m+[m[32m     */[m
[32m+[m[32m    public CardPaymentController() {[m
[32m+[m[32m        super();[m
[32m+[m[32m        // TODO Auto-generated constructor stub[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * @see HttpServlet#doGet(HttpServletRequest request, HttpServletResponse response)[m
[32m+[m	[32m */[m
[32m+[m	[32mprotected void doGet(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {[m
[32m+[m[41m	[m
[32m+[m		[32mint accNo=Integer.parseInt(request.getParameter("accNo"));[m
[32m+[m		[32mint cvv=Integer.parseInt(request.getParameter("cvv"));[m
[32m+[m		[32mint ammount=Integer.parseInt(request.getParameter("ammount"));[m
[32m+[m[41m		[m
[32m+[m		[32m HttpSession session=request.getSession();[m
[32m+[m		[32m    List<Customer> custList=(List<Customer>)session.getAttribute("user");[m
[32m+[m		[32m    if(custList != null && !custList.isEmpty()) {[m
[32m+[m		[32m        Customer c=custList.get(0);[m
[32m+[m		[32m       List<Card>cardList=new ArrayList<>();[m
[32m+[m		[32m        BankDao bdao=new BankDaoImpl();[m
[32m+[m		[32m        Card card=new Card(c.getAccNo(), accNo, cvv, "Card", ammount);[m
[32m+[m		[32m        cardList.add(card);[m
[32m+[m		[32m        boolean b=bdao.validateReciever(accNo);[m
[32m+[m		[32m        if(b==true) {[m
[32m+[m		[32m            int senderAccBal=bdao.accountBalance(c.getAccNo());[m
[32m+[m		[32m            int recieverBal=bdao.accountBalance(accNo);[m
[32m+[m		[32m            if(senderAccBal > ammount) {[m
[32m+[m		[32m                int j=bdao.updateBalance(c.getAccNo(), senderAccBal-(int)ammount);[m
[32m+[m		[32m                int k=bdao.updateBalance(accNo, recieverBal+(int)ammount);[m
[32m+[m		[32m                int i=bdao.cardTransaction(cardList);[m
[32m+[m
[32m+[m		[32m                if(i > 0 && j > 0 && k > 0) {[m
[32m+[m		[32m                    session.setAttribute("CardError", "Transaction Successful...!!!");[m
[32m+[m		[32m                    response.sendRedirect("CardPayment.jsp");[m
[32m+[m		[32m                } else {[m
[32m+[m		[32m                    session.setAttribute("CardError", "Transaction Failed...!!!");[m
[32m+[m		[32m                    response.sendRedirect("CardPayment.jsp");[m
[32m+[m		[32m                }[m
[32m+[m		[32m            } else {[m
[32m+[m		[32m                session.setAttribute("CardError", "Insufficient Funds...!!!");[m
[32m+[m		[32m                response.sendRedirect("CardPayment.jsp");[m
[32m+[m		[32m            }[m
[32m+[m		[32m        } else {[m
[32m+[m		[32m            session.setAttribute("CardError", "Invalid Account Number...!!!");[m
[32m+[m		[32m            response.sendRedirect("CardPayment.jsp");[m
[32m+[m		[32m        }[m
[32m+[m		[32m    } else {[m
[32m+[m		[32m        session.setAttribute("CardError", "User details not found...!!!");[m
[32m+[m		[32m        response.sendRedirect("CardPayment.jsp");[m
[32m+[m		[32m    }[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * @see HttpServlet#doPost(HttpServletRequest request, HttpServletResponse response)[m
[32m+[m	[32m */[m
[32m+[m	[32mprotected void doPost(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {[m
[32m+[m		[32m// TODO Auto-generated method stub[m
[32m+[m		[32mdoGet(request, response);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m}[m
