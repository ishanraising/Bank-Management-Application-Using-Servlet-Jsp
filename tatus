[32m+[m[32m@WebServlet("/NeftController")[m
[32m+[m[32mpublic class NeftController extends HttpServlet {[m
[32m+[m	[32mprivate static final long serialVersionUID = 1L;[m
[32m+[m[41m       [m
[32m+[m[32m    /**[m
[32m+[m[32m     * @see HttpServlet#HttpServlet()[m
[32m+[m[32m     */[m
[32m+[m[32m    public NeftController() {[m
[32m+[m[32m        super();[m
[32m+[m[32m        // TODO Auto-generated constructor stub[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * @see HttpServlet#doGet(HttpServletRequest request, HttpServletResponse response)[m
[32m+[m	[32m */[m
[32m+[m	[32mprotected void doGet(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {[m
[32m+[m		[32mint recieverAccNo=Integer.parseInt(request.getParameter("toAcNo"));[m
[32m+[m	[32m    String ifsc=request.getParameter("ifsc");[m
[32m+[m	[32m    String branch=request.getParameter("branch");[m
[32m+[m	[32m    float ammount=Float.parseFloat(request.getParameter("ammount"));[m
[32m+[m
[32m+[m	[32m    HttpSession session=request.getSession();[m
[32m+[m	[32m    List<Customer> custList=(List<Customer>)session.getAttribute("user");[m
[32m+[m	[32m    if(custList != null && !custList.isEmpty()) {[m
[32m+[m	[32m        Customer c=custList.get(0);[m
[32m+[m	[32m        List<Neft>neftList=new ArrayList<Neft>();[m
[32m+[m	[32m        BankDao bdao=new BankDaoImpl();[m
[32m+[m	[32m        Neft n=new Neft(c.getAccNo(), recieverAccNo, ifsc, branch, "Neft", ammount);[m
[32m+[m	[32m        neftList.add(n);[m
[32m+[m	[32m        boolean b=bdao.validateReciever(recieverAccNo);[m
[32m+[m	[32m        if(b==true) {[m
[32m+[m	[32m            int senderAccBal=bdao.accountBalance(c.getAccNo());[m
[32m+[m	[32m            int recieverBal=bdao.accountBalance(recieverAccNo);[m
[32m+[m	[32m            if(senderAccBal > ammount) {[m
[32m+[m	[32m                int j=bdao.updateBalance(c.getAccNo(), senderAccBal-(int)ammount);[m
[32m+[m	[32m                int k=bdao.updateBalance(recieverAccNo, recieverBal+(int)ammount);[m
[32m+[m	[32m                int i=bdao.neftTransaction(neftList);[m
[32m+[m
[32m+[m	[32m                if(i > 0 && j > 0 && k > 0) {[m
[32m+[m	[32m                    session.setAttribute("neftError", "Transaction Successful...!!!");[m
[32m+[m	[32m                    response.sendRedirect("Neft.jsp");[m
[32m+[m	[32m                } else {[m
[32m+[m	[32m                    session.setAttribute("neftError", "Transaction Failed...!!!");[m
[32m+[m	[32m                    response.sendRedirect("Neft.jsp");[m
[32m+[m	[32m                }[m
[32m+[m	[32m            } else {[m
[32m+[m	[32m                session.setAttribute("neftError", "Insufficient Funds...!!!");[m
[32m+[m	[32m                response.sendRedirect("Neft.jsp");[m
[32m+[m	[32m            }[m
[32m+[m	[32m        } else {[m
[32m+[m	[32m            session.setAttribute("neftError", "Invalid Account Number...!!!");[m
[32m+[m	[32m            response.sendRedirect("Neft.jsp");[m
[32m+[m	[32m        }[m
[32m+[m	[32m    } else {[m
[32m+[m	[32m        session.setAttribute("neftError", "User details not found...!!!");[m
[32m+[m	[32m        response.sendRedirect("Neft.jsp");[m
[32m+[m	[32m    }[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
