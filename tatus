[32m+[m[32mpackage project.controller;[m
[32m+[m
[32m+[m[32mimport java.io.Closeable;[m
[32m+[m
[32m+[m[32mimport java.io.IOException;[m
[32m+[m[32mimport java.io.OutputStream;[m
[32m+[m[32mimport java.util.ArrayList;[m
[32m+[m[32mimport java.util.List;[m
[32m+[m
[32m+[m[32mimport javax.servlet.ServletException;[m
[32m+[m[32mimport javax.servlet.annotation.WebServlet;[m
[32m+[m[32mimport javax.servlet.http.HttpServlet;[m
[32m+[m[32mimport javax.servlet.http.HttpServletRequest;[m
[32m+[m[32mimport javax.servlet.http.HttpServletResponse;[m
[32m+[m[32mimport javax.servlet.http.HttpSession;[m
[32m+[m[32mimport javax.xml.crypto.dsig.spec.XSLTTransformParameterSpec;[m
[32m+[m
[32m+[m[32mimport org.apache.poi.hssf.model.Sheet;[m
[32m+[m[32mimport org.apache.poi.hssf.model.Workbook;[m
[32m+[m[32mimport org.apache.poi.hssf.record.formula.functions.Row;[m
[32m+[m[32mimport org.apache.poi.hssf.usermodel.HSSFCell;[m
[32m+[m[32mimport org.apache.poi.hssf.usermodel.HSSFRow;[m
[32m+[m[32mimport org.apache.poi.hssf.usermodel.HSSFSheet;[m
[32m+[m[32mimport org.apache.poi.hssf.usermodel.HSSFWorkbook;[m
[32m+[m[32mimport org.apache.tomcat.util.http.fileupload.ByteArrayOutputStream;[m
[32m+[m
[32m+[m[32mimport project.dao.BankDao;[m
[32m+[m[32mimport project.dao.BankDaoImpl;[m
[32m+[m[32mimport project.model.Customer;[m
[32m+[m[32mimport project.model.Transaction;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Servlet implementation class BankStatementController[m
[32m+[m[32m */[m
[32m+[m[32m@WebServlet("/BankStatementController")[m
[32m+[m[32mpublic class BankStatementController extends HttpServlet {[m
[32m+[m	[32mprivate static final long serialVersionUID = 1L;[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * @see HttpServlet#HttpServlet()[m
[32m+[m	[32m */[m
[32m+[m	[32mpublic BankStatementController() {[m
[32m+[m		[32msuper();[m
[32m+[m		[32m// TODO Auto-generated constructor stub[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * @see HttpServlet#doGet(HttpServletRequest request, HttpServletResponse response)[m
[32m+[m	[32m */[m
[32m+[m	[32mprotected void doGet(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {[m
[32m+[m		[32mList<Customer>custList=null;[m
[32m+[m		[32mList<Transaction>transList=new ArrayList<Transaction>();[m
[32m+[m		[32mCustomer c=null;[m
[32m+[m		[32mBankDao bdao=new BankDaoImpl();[m
[32m+[m		[32mHttpSession session=request.getSession(true);[m
[32m+[m		[32mcustList=(List<Customer>)session.getAttribute("user");[m
[32m+[m		[32mif (custList != null && !custList.isEmpty()) {[m
[32m+[m			[32mc = custList.get(0);[m
[32m+[m		[32m} else {[m
[32m+[m			[32mSystem.out.println("No data in List");[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mtransList=bdao.bankStatement(c.getAccNo());[m
[32m+[m		[32mHSSFWorkbook workbook = new HSSFWorkbook();[m
[32m+[m		[32mHSSFSheet sheet = workbook.createSheet("Transactions");[m
[32m+[m		[32mHSSFRow headerRow = sheet.createRow(0);[m
[32m+[m		[32mString[] columns = {"Sender Account Number", "Reciever Account Number", "Transaction Type", "Ammount"};[m
[32m+[m		[32mfor (int i = 0; i < columns.length; i++) {[m
[32m+[m			[32mHSSFCell cell = headerRow.createCell((short) i);[m
[32m+[m			[32mcell.setCellValue(columns[i]);[m
[32m+[m		[32m}[m
[32m+[m		[32mint rowNum = 1;[m
[32m+[m		[32mfor (Transaction t : transList) {[m
[32m+[m			[32mHSSFRow row = sheet.createRow(rowNum++);[m
[32m+[m			[32mrow.createCell((short) 0).setCellValue(t.getSenderAccNo());[m
[32m+[m			[32mrow.createCell((short) 1).setCellValue(t.getRecieverAccNo());[m
[32m+[m			[32mrow.createCell((short) 2).setCellValue(t.getTransType());[m
[32m+[m			[32mrow.createCell((short) 3).setCellValue(t.getAmmount());[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m		[32mByteArrayOutputStream outputStream = new ByteArrayOutputStream();[m
[32m+[m		[32mworkbook.write(outputStream);[m
[32m+[m
[32m+[m		[32mresponse.setContentType("application/vnd.ms-excel");[m
[32m+[m		[32mresponse.setHeader("Content-Disposition", "attachment; filename=transactions.xls");[m
[32m+[m
[32m+[m
[32m+[m		[32mOutputStream out = response.getOutputStream();[m
[32m+[m		[32moutputStream.writeTo(out);[m
[32m+[m		[32mout.close();[m
[32m+[m
[32m+[m		[32m//		    session.setAttribute("bankstatement", transList);[m
[32m+[m		[32m//		   response.sendRedirect("BankStatement.jsp");[m
[32m+[m		[32m//[m[41m		   [m
[32m+[m
[32m+[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * @see HttpServlet#doPost(HttpServletRequest request, HttpServletResponse response)[m
[32m+[m	[32m */[m
[32m+[m	[32mprotected void doPost(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {[m
[32m+[m		[32m// TODO Auto-generated method stub[m
[32m+[m		[32mdoGet(request, response);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m}[m
